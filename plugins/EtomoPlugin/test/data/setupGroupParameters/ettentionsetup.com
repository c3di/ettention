# Command file for running ettentionsetup created by makecomfile
$ettentionsetup -StandardInput
CommandFile	tilt.com
algorithm.numberOfIterations 3
algorithm.lambda	0.3
algorithm	blockIterative
algorithm.blockSize 7
algorithm.useLongObjectCompensation	true
debug.outputKernelParameters	true	
debug.displayProjectionRMS	true
debug.writeProjections	true
debug.writeProjectionsInFourierSpace	true
debug.writeVirtualProjections	true
debug.writeVirtualProjectionsInFourierSpace	true
debug.writeResiduals	true
debug.writeRayLength	true
debug.writeIntermediateVolumes	true
debug.writeIntermediateVolumesAfterIterations	true
debug.infoPath	folderWithDebugFiles
debug.profilingLevel	detail
debug.performanceReportFile performaceFile.csv
hardware.openclVendor	intel
hardware.openclDeviceType	cpu_gpu
hardware.disableImageSupport	true
hardware.subVolumeCount	5
priorknowledge.volumeMinimumIntensity	1.0
priorknowledge.volumeMaximumIntensity	100.0
forwardProjection.samples 16
backProjection.samples	9
volume.origin	-50.0,70.0,0.0
volume.dimension 200.0,300.0,40.0
volume.resolution 100,150,20
volume.initFile initialVolume.mrc
output.voxelType unsigned16
output.orientation xyz_order
output.invert	true